{
    "container_image": "fedora:37",
    "package_manager": "dnf",
    "microsoft_repo": "https://packages.microsoft.com/config/rhel/8/prod.repo",
    "build_deps": [
        "cmake",
        "gcc",
        "git",
        "krb5-devel",
        "make",
        "openssl",
        "openssl-devel",
        "which"
    ],
    "test_deps": [
        "gssntlmssp",
        "krb5-devel",
        "krb5-workstation",
        "openssl",
        "powershell",
        "which"
    ],
    "cert_staging_dir": "/etc/pki/ca-trust/source/anchors",
    "cert_staging_cmd": "update-ca-trust extract"
}
